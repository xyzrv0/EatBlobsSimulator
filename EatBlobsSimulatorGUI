local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Eat Blobs Simulator", "Midnight")

-- Skins
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewButton("Get free skin GALAXY", "buy the galaxy skin for free", function()
    local args = {
        [1] = "Galaxy",
        [2] = 0
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("BuySkin"):FireServer(unpack(args))    
end)

MainSection:NewButton("Get free skin CODE", "buy the code skin for free", function()
