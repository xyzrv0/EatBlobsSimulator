--Extras

local Extras = Window:NewTab("Extras")
local ExtrasSection = Extras:NewSection("Extras")

ExtrasSection:NewToggle("Collect Blobs Faster", "walk faster", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 400
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

ExtrasSection:NewButton("GodMode", "godmode", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/zephyr10101/ignore-touchinterests/main/main",true))()
end)

























